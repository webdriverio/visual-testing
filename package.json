{
  "name": "@wdio/visual-testing-monorepo",
  "author": "Wim Selles - wswebcreation",
  "description": "Visual testing monorepo for WebdriverIO",
  "license": "MIT",
  "type": "module",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/webdriverio-community/visual-testing.git"
  },
  "scripts": {
    "build": "pnpm run -r build",
    "clean": "rimraf coverage build .tmp",
    "release": "run-s build && changeset publish",
    "test": "run-s test:*",
    "test:lint": "eslint --cache packages tests",
    "test:unit": "vitest --coverage --run",
    "test.local.init": "rimraf localBaseline && wdio ./tests/configs/wdio.local.init.conf.ts",
    "test.local.desktop": "wdio tests/configs/wdio.local.desktop.conf.ts",
    "test.local.appium": "wdio tests/configs/wdio.local.appium.conf.ts",
    "test.local.desktop.multi": "wdio tests/configs/wdio.local.desktop.multi.conf.ts",
    "test.unit.coverage": "vitest --coverage",
    "test.saucelabs": "wdio ./tests/configs/wdio.saucelabs.conf.ts",
    "test.saucelabs.desktop": "SAUCE_ENV=desktop wdio ./tests/configs/wdio.saucelabs.conf.ts",
    "test.saucelabs.emu": "SAUCE_ENV=emu wdio ./tests/configs/wdio.saucelabs.conf.ts",
    "test.saucelabs.sims": "SAUCE_ENV=sims wdio ./tests/configs/wdio.saucelabs.conf.ts",
    "prepare": "husky install",
    "watch": "pnpm run -r --parallel watch"
  },
  "dependencies": {
    "@wdio/visual-service": "workspace:^"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@tsconfig/node20": "^20.1.2",
    "@types/eslint": "^8.56.2",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@typescript-eslint/utils": "^6.18.1",
    "@vitest/coverage-v8": "^1.1.3",
    "@wdio/cli": "^8.27.1",
    "@wdio/globals": "^8.27.0",
    "@wdio/local-runner": "^8.27.0",
    "@wdio/mocha-framework": "^8.27.0",
    "@wdio/sauce-service": "^8.27.0",
    "@wdio/shared-store-service": "^8.27.2",
    "@wdio/spec-reporter": "^8.27.0",
    "@wdio/types": "^8.27.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unicorn": "^50.0.1",
    "eslint-plugin-wdio": "^8.24.12",
    "husky": "^8.0.3",
    "jsdom": "^23.2.0",
    "npm-run-all": "^4.1.5",
    "release-it": "^17.0.1",
    "rimraf": "^5.0.5",
    "saucelabs": "^7.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3",
    "webdriverio": "^8.27.0"
  }
}
